# HyperEVM RPC endpoint (keep your real key private)
HYPER_RPC_URL="https://hyperliquid-testnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY"

# HypurrFi reserve asset (update with the live USDC token on HyperEVM testnet)
USDC_ASSET=0x0000000000000000000000000000000000000000

# Vault parameters (basis points)
TARGET_LTV_BPS=6000
MAX_LTV_BPS=7500
REBALANCE_THRESHOLD_BPS=500

# Optional custom vault owner (defaults to deployer when unset)
VAULT_OWNER=0x0000000000000000000000000000000000000000

# Local keystore configuration (path is relative to contracts/ directory)
DEPLOYER_KEYSTORE_PATH=wallets/hypurr-deployer.json
DEPLOYER_KEY_ALIAS=hypurr-deployer

